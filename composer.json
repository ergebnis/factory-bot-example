{
  "name": "ergebnis/factory-bot-example",
  "description": "Provides a GitHub repository template for a PHP library, using GitHub actions.",
  "license": "MIT",
  "type": "project",
  "keywords": [
    "ergebnis",
    "factory-bot",
    "example"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/factory-bot-example",
  "support": {
    "issues": "https://github.com/ergebnis/factory-bot-example/issues",
    "source": "https://github.com/ergebnis/factory-bot-example"
  },
  "require": {
    "php": "^7.4.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-pdo_pgsql": "*",
    "doctrine/annotations": "^1.12.1",
    "doctrine/collections": "^1.6.8",
    "doctrine/doctrine-bundle": "^2.2.3",
    "doctrine/doctrine-migrations-bundle": "^3.0.2",
    "doctrine/migrations": "^3.4.1",
    "doctrine/orm": "^2.7.5",
    "doctrine/persistence": "^1.3.3",
    "ramsey/uuid": "^4.2.3",
    "sensio/framework-extra-bundle": "^6.2.1",
    "symfony/config": "^5.2.4",
    "symfony/console": "^5.2.4",
    "symfony/dependency-injection": "^5.2.12",
    "symfony/dotenv": "^5.2.4",
    "symfony/error-handler": "^5.3.7",
    "symfony/flex": "^1.18.5",
    "symfony/framework-bundle": "^5.2.4",
    "symfony/http-kernel": "^5.2.4",
    "symfony/routing": "^5.2.4",
    "symfony/serializer": "^5.3.12",
    "symfony/yaml": "^5.2.4"
  },
  "require-dev": {
    "ext-pdo_sqlite": "*",
    "dama/doctrine-test-bundle": "^6.7.5",
    "ergebnis/composer-normalize": "^2.24.0",
    "ergebnis/factory-bot": "~0.3.1",
    "ergebnis/license": "^1.2.0",
    "ergebnis/php-cs-fixer-config": "^2.13.0",
    "ergebnis/phpstan-rules": "~0.15.3",
    "ergebnis/test-util": "^1.3.0",
    "jangregor/phpstan-prophecy": "~0.8.1",
    "phpstan/extension-installer": "^1.1.0",
    "phpstan/phpstan": "~0.12.99",
    "phpstan/phpstan-deprecation-rules": "~0.12.6",
    "phpstan/phpstan-doctrine": "~0.12.44",
    "phpstan/phpstan-phpunit": "~0.12.22",
    "phpstan/phpstan-strict-rules": "~0.12.11",
    "phpstan/phpstan-symfony": "~0.12.44",
    "phpunit/phpunit": "^9.5.18",
    "psalm/plugin-phpunit": "~0.12.2",
    "psalm/plugin-symfony": "^1.5.0",
    "symplify/config-transformer": "^10.1.2",
    "vimeo/psalm": "^3.18.2",
    "weirdan/doctrine-psalm-plugin": "~0.11.3"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Example\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Example\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true
    },
    "platform": {
      "php": "7.4.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^5.1.0"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
