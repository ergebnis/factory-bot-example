{
  "name": "ergebnis/factory-bot-example",
  "description": "Provides a GitHub repository template for a PHP library, using GitHub actions.",
  "license": "MIT",
  "type": "project",
  "keywords": [
    "ergebnis",
    "factory-bot",
    "example"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com",
      "homepage": "https://localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/factory-bot-example",
  "support": {
    "issues": "https://github.com/ergebnis/factory-bot-example/issues",
    "source": "https://github.com/ergebnis/factory-bot-example"
  },
  "require": {
    "php": "~8.3.0",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-pdo_pgsql": "*",
    "doctrine/collections": "^2.4.0",
    "doctrine/doctrine-bundle": "^2.17.1",
    "doctrine/doctrine-migrations-bundle": "^3.7.0",
    "doctrine/migrations": "^3.9.5",
    "doctrine/orm": "^3.6.0",
    "doctrine/persistence": "^3.4.0",
    "ramsey/uuid": "^4.9.2",
    "symfony/config": "^7.3.6",
    "symfony/console": "^7.3.6",
    "symfony/dependency-injection": "^7.3.6",
    "symfony/dotenv": "^7.3.2",
    "symfony/error-handler": "^7.3.6",
    "symfony/filesystem": "^7.3.2",
    "symfony/flex": "^2.10.0",
    "symfony/framework-bundle": "^7.3.5",
    "symfony/http-kernel": "^7.3.7",
    "symfony/routing": "^7.3.6",
    "symfony/runtime": "^7.3.4",
    "symfony/serializer": "^7.3.5",
    "symfony/yaml": "^7.3.5"
  },
  "require-dev": {
    "ext-pdo_sqlite": "*",
    "dama/doctrine-test-bundle": "^8.4.0",
    "ergebnis/composer-normalize": "^2.48.2",
    "ergebnis/factory-bot": "^1.10.0",
    "ergebnis/license": "^2.7.0",
    "ergebnis/php-cs-fixer-config": "^6.58.0",
    "ergebnis/phpunit-slow-test-detector": "^2.20.0",
    "infection/infection": "~0.27.11",
    "phpunit/phpunit": "^10.5.26",
    "psalm/plugin-phpunit": "~0.19.0",
    "psalm/plugin-symfony": "^5.2.6",
    "rector/rector": "^2.2.5",
    "symplify/config-transformer": "^12.3.4",
    "vimeo/psalm": "^5.26.1",
    "weirdan/doctrine-psalm-plugin": "^2.9.0"
  },
  "replace": {
    "paragonie/random_compat": "*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*",
    "symfony/polyfill-php80": "*",
    "symfony/polyfill-php81": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "App\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true,
      "symfony/runtime": true
    },
    "audit": {
      "abandoned": "report"
    },
    "platform": {
      "php": "8.3.8"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^5.1.0"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
