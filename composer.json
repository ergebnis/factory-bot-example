{
  "name": "ergebnis/factory-bot-example",
  "description": "Provides a GitHub repository template for a PHP library, using GitHub actions.",
  "license": "MIT",
  "type": "project",
  "keywords": [
    "ergebnis",
    "factory-bot",
    "example"
  ],
  "authors": [
    {
      "name": "Andreas MÃ¶ller",
      "email": "am@localheinz.com"
    }
  ],
  "homepage": "https://github.com/ergebnis/factory-bot-example",
  "support": {
    "issues": "https://github.com/ergebnis/factory-bot-example/issues",
    "source": "https://github.com/ergebnis/factory-bot-example"
  },
  "require": {
    "php": "^8.1",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-pdo_pgsql": "*",
    "doctrine/annotations": "^1.13.3",
    "doctrine/collections": "^1.7.3",
    "doctrine/doctrine-bundle": "^2.9.1",
    "doctrine/doctrine-migrations-bundle": "^3.2.2",
    "doctrine/migrations": "^3.5.0",
    "doctrine/orm": "^2.11.2",
    "doctrine/persistence": "^2.5.4",
    "ramsey/uuid": "^4.5.1",
    "sensio/framework-extra-bundle": "^6.2.9",
    "symfony/config": "^6.1.3",
    "symfony/console": "^6.1.7",
    "symfony/dependency-injection": "^6.1.5",
    "symfony/dotenv": "^6.2.0",
    "symfony/error-handler": "^6.1.6",
    "symfony/flex": "^2.2.3",
    "symfony/framework-bundle": "^5.2.4",
    "symfony/http-kernel": "^6.1.12",
    "symfony/routing": "^6.1.7",
    "symfony/serializer": "^6.1.6",
    "symfony/yaml": "^6.1.6"
  },
  "require-dev": {
    "ext-pdo_sqlite": "*",
    "dama/doctrine-test-bundle": "^7.1.1",
    "ergebnis/composer-normalize": "^2.28.3",
    "ergebnis/factory-bot": "^1.0.0",
    "ergebnis/license": "^2.1.0",
    "ergebnis/php-cs-fixer-config": "^4.7.0",
    "infection/infection": "~0.26.16",
    "phpunit/phpunit": "^9.5.26",
    "psalm/plugin-phpunit": "~0.17.0",
    "psalm/plugin-symfony": "^3.1.9",
    "symplify/config-transformer": "^11.1.16",
    "vimeo/psalm": "^4.28.0",
    "weirdan/doctrine-psalm-plugin": "^2.3.0"
  },
  "replace": {
    "paragonie/random_compat": "2.*",
    "symfony/polyfill-ctype": "*",
    "symfony/polyfill-iconv": "*",
    "symfony/polyfill-php56": "*",
    "symfony/polyfill-php70": "*",
    "symfony/polyfill-php71": "*",
    "symfony/polyfill-php72": "*",
    "symfony/polyfill-php73": "*",
    "symfony/polyfill-php74": "*"
  },
  "conflict": {
    "symfony/symfony": "*"
  },
  "autoload": {
    "psr-4": {
      "Ergebnis\\Example\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ergebnis\\Example\\Test\\": "test/"
    }
  },
  "config": {
    "allow-plugins": {
      "ergebnis/composer-normalize": true,
      "infection/extension-installer": true,
      "phpstan/extension-installer": true,
      "symfony/flex": true
    },
    "platform": {
      "php": "8.1.3"
    },
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "symfony": {
      "allow-contrib": false,
      "require": "^5.1.0"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "@auto-scripts"
    ],
    "post-update-cmd": [
      "@auto-scripts"
    ],
    "auto-scripts": {
      "cache:clear": "symfony-cmd",
      "assets:install %PUBLIC_DIR%": "symfony-cmd"
    }
  }
}
